  "devDependencies": {
    "@force/eslint-config": "^0.1.2",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2"
